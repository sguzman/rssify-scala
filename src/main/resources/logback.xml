<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS z} %-5level [%thread] %logger - %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="org.github.sguzman.rss" level="DEBUG" />

  <root level="INFO">
    <appender-ref ref="STDOUT"/>
  </root>
</configuration>
